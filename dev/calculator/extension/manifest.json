{
  "name": "Scientific Calculator",
  "short_name": "Calculator",
  "version": "6.0.5",
  "description": "Scientific calculator with currency and unit conversions",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./icon-16.png",
      "19": "./icon-19.png",
      "32": "./icon-32.png",
      "38": "./icon-38.png",
      "48": "./icon-48.png",
      "96": "./icon-96.png",
      "128": "./icon-128.png",
      "512": "./icon-512.png"
    },
    "default_title": "Scientific Calculator"
  },
  "icons": {
      "16": "./icon-16.png",
      "19": "./icon-19.png",
      "32": "./icon-32.png",
      "38": "./icon-38.png",
      "48": "./icon-48.png",
      "128": "./icon-128.png",
      "512": "./icon-512.png"
  },
  "content_security_policy": "default-src 'self' https://api.exchangeratesapi.io https://www.google-analytics.com https://www.googletagmanager.com 'unsafe-eval';",
  "sandbox": {
     "pages": [
      "./core/index.html",
      "./core/style.css",
      "./core/font.tff",
      "./core/load.js",
      "./core/io.js",
      "./core/compute.js",
      "./core/log.js",
      "./core/icon-32.png"
    ]
  },
  "manifest_version": 2
}